<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
  <metadata>
    <id>assemblyinfo</id>
    <version>0.1.0</version>
    <title>assemblyinfo</title>
    <authors>Peter Sunde</authors>
    <owners>petersunde</owners>
    <projectUrl>https://github.com/peters/assemblyinfo</projectUrl>
    <requireLicenseAcceptance>true</requireLicenseAcceptance>
    <licenseUrl>https://raw.github.com/peters/assemblyinfo/develop/LICENSE</licenseUrl>
    <description>Inspect a .NET assembly (safely) using Mono.Cecil</description>
    <summary>
      This small library uses Mono cecil to inspect MSIL code found in .NET assemblies
      without invoking `Assembly.Load` which enables you to avoid having to load
      the assembly's dependencies.
    </summary>
    <copyright>Peter Sunde</copyright>
    <tags>Mono Cecil Assembly</tags>
  </metadata>
